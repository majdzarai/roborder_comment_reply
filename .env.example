# OpenRouter API Key (required)
ANTHROPIC_API_KEY=sk-or-v1-your-openrouter-key-here

# Client API Keys (comma-separated for multiple clients)
# Generate secure keys and give one to each client
API_KEYS=roborder_prod_abc123xyz,client_key_here

# Redis Configuration
REDIS_URL=redis://localhost:6379

# CORS Configuration
ALLOWED_ORIGINS=https://app.roborder.ai

# Logging
LOG_LEVEL=INFO

# Reply Generation Settings
MAX_TOKENS_PER_REPLY=200
